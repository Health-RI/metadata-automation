@prefix dash: <http://datashapes.org/dash#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix healthdcatap: <http://healthdataportal.eu/ns/health#> .
@prefix hri: <http://data.health-ri.nl/core/p2/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

hri:AgentShape a sh:NodeShape ;
    sh:property [ dash:editor dash:TextAreaEditor ;
            dash:viewer dash:LiteralViewer ;
            sh:datatype xsd:string ;
            sh:description "A name for some thing." ;
            sh:minCount 1 ;
            sh:name "name" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path foaf:name ],
        [ dash:editor dash:URIEditor ;
            dash:viewer dash:URIViewer ;
            sh:description "A homepage for some thing." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "URL" ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path foaf:homepage ],
        [ dash:editor dash:TextAreaEditor ;
            dash:viewer dash:LiteralViewer ;
            sh:datatype xsd:string ;
            sh:description "A description of the publisher activities." ;
            sh:maxCount 1 ;
            sh:name "publisher note" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path healthdcatap:publishernote ],
        [ dash:editor dash:URIEditor ;
            dash:viewer dash:URIViewer ;
            sh:description "Spatial characteristics of the resource." ;
            sh:name "country" ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path dct:spatial ],
        [ dash:editor dash:URIEditor ;
            dash:viewer dash:URIViewer ;
            sh:description "A email address via which contact can be made. This property SHOULD be used to provide the email address of the Agent, specified using fully qualified mailto: URI scheme [RFC6068]. The email SHOULD be used to establish a communication channel to the agent." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "email" ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path foaf:mbox ;
            sh:pattern "^mailto:.+@.+\\..+$" ],
        [ dash:editor dash:TextAreaEditor ;
            dash:viewer dash:LiteralViewer ;
            sh:datatype xsd:string ;
            sh:description "An unambiguous reference to the resource within a given context." ;
            sh:minCount 1 ;
            sh:name "identifier" ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path dct:identifier ],
        [ dash:editor dash:URIEditor ;
            dash:viewer dash:URIViewer ;
            sh:description "A type of organisation that makes the Dataset available." ;
            sh:maxCount 1 ;
            sh:name "publisher type" ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path healthdcatap:publishertype ],
        [ dash:editor dash:URIEditor ;
            dash:viewer dash:URIViewer ;
            sh:description "The nature or genre of the resource." ;
            sh:maxCount 1 ;
            sh:name "type" ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path dct:type ] ;
    sh:targetClass hri:Agent,
        foaf:Agent .

