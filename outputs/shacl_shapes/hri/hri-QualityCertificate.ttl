@prefix dash: <http://datashapes.org/dash#> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix hri: <http://data.health-ri.nl/core/p2/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

hri:QualityCertificateShape a sh:NodeShape ;
    sh:property [ dash:editor dash:URIEditor ;
            dash:viewer dash:URIViewer ;
            sh:description "The object of the relationship is a resource that is a body of the Annotation." ;
            sh:maxCount 1 ;
            sh:name "body" ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path <oa:hasBody> ],
        [ dash:editor dash:URIEditor ;
            dash:viewer dash:URIViewer ;
            sh:description "The relationship between an Annotation and its Target." ;
            sh:maxCount 1 ;
            sh:name "target" ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path <oa:hasTarget> ] ;
    sh:targetClass hri:QualityCertificate,
        dqv:QualityCertificate .

