hri:aux-RelationshipShape a owl:Ontology;
  rdfs:label "Excel template for Relationship class"@en;
  rdfs:comment "Excel template for Relationship class"@en;
  dct:description "This is an excel template for Relationship class in Health RI Core plateau 2."@en;
  owl:versionInfo "0.1" .

hri:RelationshipShape a sh:NodeShape;
  rdfs:label "Relationship"@en;
  sh:targetClass dcat:Relationship;
  sh:property <http://data.health-ri.nl/core/p2/RelationshipShape#dcat:hadRole>, <http://data.health-ri.nl/core/p2/RelationshipShape#dct:relation> .

<http://data.health-ri.nl/core/p2/RelationshipShape#dcat:hadRole> sh:path dcat:hadRole;
  sh:name "had role"@en;
  sh:description "The function of an entity or agent with respect to another entity or resource."@en;
  sh:minCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://data.health-ri.nl/core/p2/RelationshipShape#dct:relation> sh:path dct:relation;
  sh:name "relation"@en;
  sh:description "A related resource."@en;
  sh:minCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .
