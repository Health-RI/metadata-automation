foaf:AgentShape a owl:Ontology, sh:NodeShape;
  rdfs:label "Excel template for Agent class"@en, "Agent"@en;
  rdfs:comment "Excel template for Agent class"@en;
  dct:description "This is an excel template for Agent class in Health RI Core plateau 2."@en;
  owl:versionInfo "0.1";
  sh:property <http://xmlns.com/foaf/0.1/AgentShape#country>, <http://xmlns.com/foaf/0.1/AgentShape#email>,
    <http://xmlns.com/foaf/0.1/AgentShape#identifier>, <http://xmlns.com/foaf/0.1/AgentShape#name>,
    <http://xmlns.com/foaf/0.1/AgentShape#publisher-note>, <http://xmlns.com/foaf/0.1/AgentShape#publisher-type>,
    <http://xmlns.com/foaf/0.1/AgentShape#type>, <http://xmlns.com/foaf/0.1/AgentShape#url> .

<http://xmlns.com/foaf/0.1/AgentShape#country> sh:path dct:spatial;
  sh:name "country"@en;
  sh:description "Spatial characteristics of the resource."@en;
  sh:minCount 0;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://xmlns.com/foaf/0.1/AgentShape#email> sh:path foaf:mbox;
  sh:name "email"@en;
  sh:description "A email address via which contact can be made. This property SHOULD be used to provide the email address of the Agent, specified using fully qualified mailto: URI scheme [RFC6068]. The email SHOULD be used to establish a communication channel to the agent."@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  sh:pattern "^mailto:.+@.+\\..+$";
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://xmlns.com/foaf/0.1/AgentShape#identifier> sh:path dct:identifier;
  sh:name "identifier"@en;
  sh:description "An unambiguous reference to the resource within a given context."@en;
  sh:minCount 1;
  sh:nodeKind sh:Literal;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextAreaEditor .

<http://xmlns.com/foaf/0.1/AgentShape#name> sh:path foaf:name;
  sh:name "name"@en;
  sh:description "A name for some thing."@en;
  sh:minCount 1;
  sh:nodeKind sh:Literal;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextAreaEditor .

<http://xmlns.com/foaf/0.1/AgentShape#publisher-note> sh:path healthdcatap:publishernote;
  sh:name "publisher note"@en;
  sh:description "A description of the publisher activities."@en;
  sh:minCount 0;
  sh:maxCount 1;
  sh:nodeKind sh:Literal;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextAreaEditor .

<http://xmlns.com/foaf/0.1/AgentShape#publisher-type> sh:path healthdcatap:publishertype;
  sh:name "publisher type"@en;
  sh:description "A type of organisation that makes the Dataset available."@en;
  sh:minCount 0;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://xmlns.com/foaf/0.1/AgentShape#type> sh:path dct:type;
  sh:name "type"@en;
  sh:description "The nature or genre of the resource."@en;
  sh:minCount 0;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://xmlns.com/foaf/0.1/AgentShape#url> sh:path foaf:homepage;
  sh:name "URL"@en;
  sh:description "A homepage for some thing."@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .
