            sh:path dcat1:keyword ;
            "rdf_term"^^xsd:string "DCAT.keyword"^^xsd:string ;
            "rdf_type"^^xsd:string "rdfs_literal"^^xsd:string ],
        [ sh:datatype xsd:anyURI ;
