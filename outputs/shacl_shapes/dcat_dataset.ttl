                        sh:nodeKind sh:Literal ] ) ;
            sh:order 35 ;
            sh:path dcatv3:version ;
            "rdf_term"^^xsd:string "DCATv3.version"^^xsd:string ;
            "rdf_type"^^xsd:string "rdfs_literal"^^xsd:string ],
