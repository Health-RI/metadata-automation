            sh:nodeKind sh:Literal ;
            sh:order 39 ;
            sh:path dcatv3:prev ;
            "rdf_term"^^xsd:string "DCATv3.prev"^^xsd:string ;
            "rdf_type"^^xsd:string "uri"^^xsd:string ],
        [ sh:datatype xsd:anyURI ;
