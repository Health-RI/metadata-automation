hri:aux-TestClassShape a owl:Ontology;
  rdfs:label "Excel template for TestClass class"@en;
  rdfs:comment "Excel template for TestClass class"@en;
  dct:description "A test class"@en;
  owl:versionInfo "0.1" .

hri:TestClassShape a sh:NodeShape;
  rdfs:label "TestClass"@en;
  sh:targetClass dcat:TestClass;
  sh:property <http://data.health-ri.nl/core/p2#TestClassShape/dct:title>, <http://data.health-ri.nl/core/p2#TestClassShape/dct:description> .

<http://data.health-ri.nl/core/p2#TestClassShape/dct:title> sh:path dct:title;
  sh:name "title"@en;
  sh:description "The title"@en;
  sh:minCount 1;
  sh:nodeKind sh:Literal;
  sh:uniqueLang true;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextFieldEditor .

<http://data.health-ri.nl/core/p2#TestClassShape/dct:description> sh:path dct:description;
  sh:name "description"@en;
  sh:description "The description"@en;
  sh:minCount 1;
  sh:nodeKind sh:Literal;
